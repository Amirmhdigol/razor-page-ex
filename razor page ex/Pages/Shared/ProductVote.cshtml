@model Tuple<int,int>
@if (User.Identity.IsAuthenticated && ViewBag.AccessDenied == null)
{
    <div class="container">

        <p>
            <a onclick="vote(true)" style="background-color:green;color:white;border-radius:16px;padding:5px;margin:3px">
                (@Model.Item1)میپسندم <img src="/img/like.png" style="width:20px;margin:3px" />
            </a>
            <a class="btn" onclick="vote(false)" style="background-color:red;color:white;border-radius:16px;padding:5px">
                (@Model.Item2)نمیپسندم  <img src="/img/dislike.png" style="width:20px;margin:3px" />
            </a>
        </p>

    </div>
}   